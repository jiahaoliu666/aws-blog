  "engineStrict": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:lambda": "tsc -p tsconfig.lambda.json && tsc-alias -p tsconfig.lambda.json",
    "build:all": "npm run build && npm run build:lambda",
    "start": "next start",
    "lint": "next lint",
    "update-news": "NODE_OPTIONS='--experimental-specifier-resolution=node' tsx src/lambda/updateNews.ts",
    "fetch-news": "NODE_OPTIONS='--experimental-specifier-resolution=node' tsx src/lambda/fetchNews.ts",
    "update-all": "NODE_OPTIONS='--experimental-specifier-resolution=node' tsx src/lambda/updateAllContent.ts",
    "clean": "rm -rf .next node_modules",
    "postinstall": "echo 'Skipping postinstall'",
    "build:layer": "./scripts/build-lambda-layer.sh",
    "deploy:lambda": "npm run build:lambda && npm run build:layer && sam deploy --guided",
    "deploy": "npm run build && npm run build:lambda && sam deploy --guided"
  },
